{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "import/extensions": 0,
    "eslint-plugin-import/prefer-default-export":"off"
  }
}
